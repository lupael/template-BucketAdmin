              %section.panel
                %header.panel-heading
                  Datetime Pickers
                  %span.tools.pull-right
                    %a.fa.fa-chevron-down{:href => "javascript:;"}
                    %a.fa.fa-cog{:href => "javascript:;"}
                    %a.fa.fa-times{:href => "javascript:;"}
                .panel-body
                  %form.form-horizontal{:action => "#"}
                    .form-group
                      %label.control-label.col-md-3 Default input Datetimepicker
                      .col-md-4
                        %input.form_datetime.form-control{:readonly => "", :size => "16", :type => "text", :value => "2012-06-15 14:45"}/
                    .form-group
                      %label.control-label.col-md-3 Component Datetimepicker
                      .col-md-4
                        .input-group.date.form_datetime-component
