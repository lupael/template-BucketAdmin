              %li.sub-menu
                %a{:href => "javascript:;"}
                  %i.fa.fa-book
                  %span UI Elements
                %ul.sub
                  %li
                    %a{:href => "general.html"} General
                  %li
                    %a{:href => "buttons.html"} Buttons
                  %li
                    %a{:href => "widget.html"} Widget
                  %li
                    %a{:href => "slider.html"} Slider
