  %body
    %section#container
      / header start
      %header.header.fixed-top.clearfix
        / logo start
        .brand
          %a.logo{:href => "index.html"}
            %img{:alt => "", :src => "images/logo.png"}/
          .sidebar-toggle-box
            .fa.fa-bars
        / logo end
        #top_menu.nav.notify-row
          / notification start
          %ul.nav.top-menu
            / settings start
            %li.dropdown
              %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
                %i.fa.fa-tasks
                %span.badge.bg-success 8
              %ul.dropdown-menu.extended.tasks-bar
                %li
                  %p You have 8 pending tasks
                %li
                  %a{:href => "#"}
                  .task-info.clearfix
                    .desc.pull-left
                      %h5 Target Sell
                      %p 25% , Deadline  12 June’13
                    %span.notification-pie-chart.pull-right{"data-percent" => "45"}
                      %span.percent
                %li
                  %a{:href => "#"}
                  .task-info.clearfix
                    .desc.pull-left
                      %h5 Product Delivery
                      %p 45% , Deadline  12 June’13
                    %span.notification-pie-chart.pull-right{"data-percent" => "78"}
                      %span.percent
                %li
                  %a{:href => "#"}
                  .task-info.clearfix
                    .desc.pull-left
                      %h5 Payment collection
                      %p 87% , Deadline  12 June’13
                    %span.notification-pie-chart.pull-right{"data-percent" => "60"}
