              %li.sub-menu
                %a{:href => "javascript:;"}
                  %i.fa.fa-envelope
                  %span Mail
                %ul.sub
                  %li
                    %a{:href => "mail.html"} Inbox
                  %li
                    %a{:href => "mail_compose.html"} Compose Mail
                  %li
                    %a{:href => "mail_view.html"} View Mail
              %li.sub-menu
                %a{:href => "javascript:;"}
                  %i.fa.fa-bar-chart-o
                  %span Charts
                %ul.sub
                  %li
                    %a{:href => "morris.html"} Morris
                  %li
                    %a{:href => "chartjs.html"} Chartjs
                  %li
                    %a{:href => "flot_chart.html"} Flot Charts
                  %li
                    %a{:href => "c3_chart.html"} C3 Chart
              %li.sub-menu
                %a{:href => "javascript:;"}
                  %i.fa.fa-bar-chart-o
                  %span Maps
                %ul.sub
                  %li
                    %a{:href => "google_map.html"} Google Map
                  %li
                    %a{:href => "vector_map.html"} Vector Map
              %li.sub-menu
                %a{:href => "javascript:;"}
                  %i.fa.fa-glass
                  %span Extra
                %ul.sub
                  %li
                    %a{:href => "blank.html"} Blank Page
                  %li
                    %a{:href => "lock_screen.html"} Lock Screen
                  %li
                    %a{:href => "profile.html"} Profile
                  %li
                    %a{:href => "invoice.html"} Invoice
                  %li
                    %a{:href => "pricing_table.html"} Pricing Table
                  %li
                    %a{:href => "timeline.html"} Timeline
                  %li
                    %a{:href => "gallery.html"} Media Gallery
                  %li
                    %a{:href => "404.html"} 404 Error
                  %li
                    %a{:href => "500.html"} 500 Error
                  %li
                    %a{:href => "registration.html"} Registration
              %li
                %a{:href => "login.html"}
                  %i.fa.fa-user
                  %span Login Page
          / sidebar menu end
      / sidebar end
      / main content start
      %section#main-content
        %section.wrapper
          / page start
          .row
            .col-md-12
              %section.panel
                %header.panel-heading
                  Toggle Switches
                  %span.tools.pull-right
                    %a.fa.fa-chevron-down{:href => "javascript:;"}
                    %a.fa.fa-cog{:href => "javascript:;"}
                    %a.fa.fa-times{:href => "javascript:;"}
                .panel-body.toggle-heading
                  %h3 Size
                  .m-bot20
                    %input.switch-large{:checked => "", :type => "checkbox"}/
                    %input{:checked => "", :type => "checkbox"}/
                    %input.switch-small{:checked => "", :type => "checkbox"}/
                    %input.switch-mini{:checked => "", :type => "checkbox"}/
                    %br/
                    %br/
                    %p Change the size programmatically:
                    %input#dimension-switch{:checked => "", :type => "checkbox"}/
                    %br/
                    %br/
                    %button#btn-size-large-switch.btn.btn-default.btn-xs Large
                    %button#btn-size-regular-switch.btn.btn-default.btn-xs Regular
                    %button#btn-size-small-switch.btn.btn-default.btn-xs Small
                    %button#btn-size-mini-switch.btn.btn-default.btn-xs Mini
                    %br/
                  %h3 Colors
                  .m-bot20
                    %input{:checked => "", "data-off" => "info", "data-on" => "primary", :type => "checkbox"}/
                    %input{:checked => "", "data-off" => "success", "data-on" => "info", :type => "checkbox"}/
                    %input{:checked => "", "data-off" => "warning", "data-on" => "success", :type => "checkbox"}/
                    %input{:checked => "", "data-off" => "danger", "data-on" => "warning", :type => "checkbox"}/
                    %input{:checked => "", "data-off" => "default", "data-on" => "danger", :type => "checkbox"}/
                    %input{:checked => "", "data-off" => "primary", "data-on" => "default", :type => "checkbox"}/
                    %br/
                    %br/
                    %span Change the colors programmatically:
                    %input#change-color-switch{:checked => "", "data-off" => "primary", "data-on" => "default", :type => "checkbox"}/
                    %br/
                    %br/
                    %button#btn-color-on-switch.btn.btn-success.btn-xs Change ON color
                    %button#btn-color-off-switch.btn.btn-danger.btn-xs Change OFF color
                  .row.m-bot20
                    .col-md-6
                      %h3 Animation
                      %div
                        %input#animated-switch{:checked => "", "data-animated" => "false", :type => "checkbox"}/
                        %br/
                        %br/
                        %button#btn-animate-switch.btn.btn-default.btn-xs Animate
                        %button#btn-dont-animate-switch.btn.btn-default.btn-xs Don't animate
                    .col-md-6
                      %h3 Disabled / Readonly
                      %div
                        %input{:checked => "", :disabled => "", :type => "checkbox"}/
                        %input{:checked => "", :readonly => "", :type => "checkbox"}/
                  .row.m-bot20
                    .col-md-6
                      %h3 Text
                      %div
                        %input#label-switch{:checked => "", "data-off-label" => "NO", "data-on-label" => "SI", :type => "checkbox"}/
                        %br/
                        %br/
                        %button#btn-label-on-switch.btn.btn-default.btn-xs Change "On" label
                        %button#btn-label-off-switch.btn.btn-default.btn-xs Change "Off" label
                    .col-md-6
                      %h3 Label Text
                      %div
                        %input{:checked => "", "data-text-label" => "TV", :type => "checkbox"}/
                  .row
                    .col-md-6
                      %h3 HTML Text
                      %div
                        %input{:checked => "", "data-off-label" => "<i class='fa fa-times'></i>", "data-on-label" => "<i class='fa fa-check'></i>", :type => "checkbox"}/
                    .col-md-6
                      %h3 HTML Text Label Icon
                      %div
                        Standard
                        %input.switch-large{:checked => "", "data-label-icon" => "fa fa-arrows-alt", "data-off-label" => "<i class='fa fa-times'></i>", "data-on-label" => "<i class='fa fa-check'></i>", :type => "checkbox"}/
                        \&nbsp;&nbsp;
                        Font Awesome
                        %input.switch-large{:checked => "", "data-label-icon" => "fa fa-youtube-square", "data-off-label" => "<i class='fa fa-hand-o-down'></i>", "data-on-label" => "<i class='fa fa-hand-o-up'></i>", :type => "checkbox"}/
                        \&nbsp;&nbsp;
                  %h3 Modal
                  %div
                    %a.btn.btn-default{"data-toggle" => "modal", :href => "#myModal", :role => "button"} Modal
                  #myModal.modal.fade{"aria-hidden" => "true", :role => "dialog", :tabindex => "-1"}
                    .modal-dialog
                      .modal-content
                        .modal-header
                          %button.close{"aria-hidden" => "true", "data-dismiss" => "modal", :type => "button"} &times;
                          %h4.modal-title Modal
                        .modal-body
                          %input{:checked => "", :type => "checkbox"}/
                        .modal-footer
                          %button.btn.btn-default{"aria-hidden" => "true", "data-dismiss" => "modal"} Close
              %section.panel
                %header.panel-heading
                  Spinner
                  %span.tools.pull-right
                    %a.fa.fa-chevron-down{:href => "javascript:;"}
                    %a.fa.fa-cog{:href => "javascript:;"}
                    %a.fa.fa-times{:href => "javascript:;"}
                .panel-body
                  %form.form-horizontal{:action => "#"}
                    .row
                      .col-md-6
                        .form-group
                          %label.control-label.col-md-3 Spinner 1
                          .col-md-5
                            #spinner1
                              .input-group.input-small
                                %input.spinner-input.form-control{:maxlength => "3", :readonly => "", :type => "text"}/
                                .spinner-buttons.input-group-btn.btn-group-vertical
                                  %button.btn.spinner-up.btn-xs.btn-default{:type => "button"}
                                    %i.fa.fa-angle-up
                                  %button.btn.spinner-down.btn-xs.btn-default{:type => "button"}
                                    %i.fa.fa-angle-down
                            %span.help-block
                              basic example
                      .col-md-6
                        .form-group
                          %label.control-label.col-md-3 Spinner 2
                          .col-md-5
                            #spinner2
                              .input-group.input-small
                                %input.spinner-input.form-control{:maxlength => "3", :readonly => "", :type => "text"}/
                                .spinner-buttons.input-group-btn.btn-group-vertical
                                  %button.btn.spinner-up.btn-xs.btn-info{:type => "button"}
                                    %i.fa.fa-angle-up
                                  %button.btn.spinner-down.btn-xs.btn-info{:type => "button"}
                                    %i.fa.fa-angle-down
                            %span.help-block
                              disabled state
                    .row
                      .col-md-6
                        .form-group
                          %label.control-label.col-md-3 Spinner 3
                          .col-md-9
                            #spinner3
                              .input-group{:style => "width:150px;"}
                                %input.spinner-input.form-control{:maxlength => "3", :readonly => "", :type => "text"}/
                                .spinner-buttons.input-group-btn
                                  %button.btn.btn-default.spinner-up{:type => "button"}
                                    %i.fa.fa-angle-up
                                  %button.btn.btn-default.spinner-down{:type => "button"}
                                    %i.fa.fa-angle-down
                            %span.help-block
                              with max value: 10
                      .col-md-6
                        .form-group
                          %label.control-label.col-md-3 Spinner 4
                          .col-md-9
                            #spinner4
                              .input-group{:style => "width:150px;"}
                                .spinner-buttons.input-group-btn
                                  %button.btn.spinner-up.btn-primary{:type => "button"}
                                    %i.fa.fa-plus
                                %input.spinner-input.form-control{:maxlength => "3", :readonly => "", :type => "text"}/
                                .spinner-buttons.input-group-btn
                                  %button.btn.spinner-down.btn-warning{:type => "button"}
                                    %i.fa.fa-minus
                            %span.help-block
                              with step: 5
          .row
            .col-md-12
              %section.panel
                %header.panel-heading
                  Date Pickers
                  %span.tools.pull-right
                    %a.fa.fa-chevron-down{:href => "javascript:;"}
